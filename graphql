DECLARE @tableName NVARCHAR(255) = '${name:csv}'; -- Multi-value handling (comma-separated values)
DECLARE @query NVARCHAR(MAX);

SET @query = 'SELECT COUNT(*) AS record FROM ' + @tableName + ' WHERE createdate >= DATEADD(MINUTE, -' + CAST(${scope} AS NVARCHAR(255)) + ', GETDATE())';

EXEC sp_executesql @query;
