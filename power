# Path to the log file
$filePath = "C:\above.txt"

# Read content from the file
$logData = Get-Content -Path $filePath -Raw

# Regular expression pattern to match names
$pattern = "(?<=for\s)(\w+)(?=\sis)"

# Extract names using regex
$names = $logData | Select-String -Pattern $pattern -AllMatches | ForEach-Object { $_.Matches.Value }

# Create JSON output
$jsonOutput = $names | ForEach-Object { @{ "{#NAME}" = $_ } } | ConvertTo-Json

# Output the result
Write-Output $jsonOutput
